
public class ContaCorrente extends Conta{
	
	private int agencia;
	private int numero;
	public ContaCorrente(Cliente cliente, double saldo) {
		super(cliente, saldo);
		
	}
	@Override
	public double exibirSaldo() {
		System.out.println("---Saldo conta Corrente--");
		return super.exibirSaldo();
	}
	
}
